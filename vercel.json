{
    "builds": [
        {
            "src": "social_site/wsgi.py",
            "use": "@vercel/python",
            "config": {"maxLambdaSize": "15bm", "runtime": "python3.9"}
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "social_site/wsgi.py"
        }
    ]
}